function select_drop(sel_value) {
  var t;

  if (sel_value == 'abuse') {
      t = xwindow(logind+'/report.php?chapter='+chapter+'&storyid='+storyid+'&title='+title,560,510);
  }
  else if (sel_value == 'c2') {
     t = xwindow(logind+'/c2_addstory.php?action=add&storyid='+storyid,560,470);
  }
}
